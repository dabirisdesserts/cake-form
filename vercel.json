{
  "version": 2,
  "functions": {
    "api/submit-order.js": {
      "runtime": "@vercel/node@3"
    },
    "api/test.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
