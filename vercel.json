{
  "version": 2,
  "builds": [
    {
      "src": "api/submit-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-order",
      "dest": "/api/submit-order.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

